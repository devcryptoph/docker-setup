{
    "tls": true,
    "tlscacert": "/etc/docker/certs/ca.pem",
    "tlscert": "/etc/dockercerts/server-cert.pem",
    "tlskey": "/etc/dockercerts/server-key.pem",
    "tlsverify": false,
    "storage-driver": "overlay2",
    "log-driver": "json-file",
    "log-opts": {
        "max-size": "100m"
    }
}
